{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/admin_toolbar": "^2.0",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/cer": "^4.0@alpha",
        "drupal/coffee": "^1.0",
        "drupal/config_ignore": "^2.1",
        "drupal/config_split": "^1.0",
        "drupal/console": "^1.0.2",
        "drupal/contact_ajax": "^1.4@RC",
        "drupal/contact_block": "^1.4",
        "drupal/core-recommended": "^8.9.6",
        "drupal/core-composer-scaffold": "^8.9.6",
        "drupal/devel": "^2.1",
        "drupal/ds": "^3.1",
        "drupal/entity_browser": "^2.1.0",
        "drupal/field_group": "^3.0",
        "drupal/google_analytics": "^3.0",
        "drupal/honeypot": "^1.23",
        "drupal/image_effects": "^2.0",
        "drupal/inline_entity_form": "^1.0@beta",
        "drupal/menu_block": "^1.5",
        "drupal/menu_link_attributes": "^1.0@beta",
        "drupal/metatag": "^1.9",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panels": "^4.2",
        "drupal/paragraphs": "^1.3",
        "drupal/paragraphs_jquery_ui_accordion": "^1.2",
        "drupal/pathauto": "^1.6",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/recaptcha": "^2.0",
        "drupal/redirect": "^1.0@beta",
        "drupal/reroute_email": "^1.1",
        "drupal/seckit": "^1.0@alpha",
        "drupal/simple_sitemap": "^3.0",
        "drupal/site_verify": "1.x-dev",
        "drupal/smtp": "^1.0@beta",
        "drupal/webform": "^5.0",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "drupal/config_exclude": "1.x-dev",
        "drupal/core-dev": "^8.9.6",
        "drupal/drupal-extension": "~3.0",
        "drupal/stage_file_proxy": "^1.1",
        "eiriksm/wait-for-listen": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin/",
        "sort-packages": true,
        "platform": {
            "php": "7.1"
        }
    },
    "scripts": {
        "import": [
            "./vendor/bin/drush --root=$(pwd)/web cc drush",
            "./vendor/bin/drush --root=$(pwd)/web cim -y",
            "./vendor/bin/drush cr"
        ],
        "export": [
            "./vendor/bin/drush --root=$(pwd)/web cex -y",
            "./vendor/bin/drush cr"
        ],
        "build": [
            "git pull",
            "@composer install --no-dev",
            "./vendor/bin/drush --root=$(pwd)/web updb -y",
            "@composer import",
            "./vendor/bin/drush --root=$(pwd)/web cr"
        ],
        "dl-files": [
            "scp -rv deploy@dev.drupal.no:public_html/dev.drupal.no/web/sites/default/files ./web/sites/default"
        ],
        "dl-db": [
            "wget --no-check-certificate http://dev.drupal.no/sanitized.db"
        ],
        "up-db": [
            "./vendor/bin/drush sql-drop --yes",
            "wget --no-check-certificate http://dev.drupal.no/sanitized.db",
            "./vendor/bin/drush sql-cli < /app/sanitized.db",
            "rm /app/sanitized.db",
            "./vendor/bin/drush cr"
        ],
        "install-site": [
            "@composer install",
            "@composer up-db"
        ],
        "uplock": [
            "@composer update --lock"
        ],
        "upnothing": [
            "@composer update nothing"
        ],
        "resetadmin": [
            "./vendor/bin/drush uli"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        },
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": {
                "Keep setting.php intact, add local config to settings.local.php": "https://github.com/zaporylie/drupal/commit/6f0b4e95f28ae945c6fa2bf0e5e81850d3e695f8.patch"
            },
            "drupal/menu_block": {
                "#2756675 - Add option to make the starting level follow the active menu item": "https://www.drupal.org/files/issues/2018-07-02/menu_block-follow-2756675-51.patch"
            },
            "drupal/ds": {
                "#2904575 - Fix layout warning": "https://www.drupal.org/files/issues/invalid_argument_in_ds_module-2904575-2.patch"
            }
        }
    }
}
